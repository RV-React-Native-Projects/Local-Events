{
  "name": "LocalEve",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "pod": "npx pod-install ios",
    "postinstall": "patch-package && npx jetify",
    "link_asset": "npx react-native-asset",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "stop": "cd android && ./gradlew --stop",
    "clear": "cd android && ./gradlew --stop && ./gradlew clean",
    "clean": "npx react-native clean",
    "watchman_clean": "watchman watch-del-all && watchman shutdown-server",
    "start_build": "pnpm link_asset && cd android && ./gradlew --stop",
    "build_apk": "ENVFILE=.env && pnpm start_build && cd android && ./gradlew assembleRelease && pnpm stop",
    "build_aab": "ENVFILE=.env && pnpm start_build && cd android && ./gradlew bundleRelease && pnpm stop",
    "android": "ENVFILE=.env && pnpm react-native run-android",
    "ios": "ENVFILE=.env && pnpm react-native run-ios --mode=Debug",
    "prepare": "husky",
    "spell-check": "cspell 'src/**/*.{js,ts,md}' --exclude 'src/constants/CountryData.ts' --config '.vscode/settings.json'",
    "plop": "plop"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-documents/picker": "^10.1.5",
    "@react-native-documents/viewer": "^1.0.1",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-navigation/bottom-tabs": "^7.4.4",
    "@react-navigation/elements": "^2.6.1",
    "@react-navigation/native": "^7.1.16",
    "@react-navigation/native-stack": "^7.3.23",
    "@react-navigation/stack": "^7.4.4",
    "@reduxjs/toolkit": "^2.8.2",
    "axios": "^1.11.0",
    "formik": "^2.4.6",
    "i18n-js": "^4.5.1",
    "lodash": "^4.17.21",
    "lottie-react-native": "^7.2.5",
    "moment": "^2.30.1",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-device-info": "^14.0.4",
    "react-native-element-dropdown": "^2.12.4",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.27.2",
    "react-native-image-crop-picker": "^0.50.1",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-localize": "^3.5.1",
    "react-native-modal": "14.0.0-rc.1",
    "react-native-outside-press": "^1.2.2",
    "react-native-pager-view": "^6.8.1",
    "react-native-permissions": "^5.4.2",
    "react-native-reanimated": "^4.0.1",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.1",
    "react-native-share": "^12.1.2",
    "react-native-size-matters": "^0.4.2",
    "react-native-skeleton-placeholder": "^5.2.4",
    "react-native-svg": "^15.12.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-worklets": "^0.4.1",
    "react-redux": "^9.2.0",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.1.1",
    "@react-native-community/cli-platform-android": "19.1.1",
    "@react-native-community/cli-platform-ios": "19.1.1",
    "@react-native/babel-preset": "0.80.2",
    "@react-native/eslint-config": "0.80.2",
    "@react-native/metro-config": "0.80.2",
    "@react-native/typescript-config": "0.80.2",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "cspell": "^9.2.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "jest": "^29.6.3",
    "jetifier": "^2.0.0",
    "plop": "^4.0.1",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18",
    "yarn": "please-use-pnpm",
    "npm": "please-use-pnpm",
    "pnpm": ">=10.14.0"
  },
  "packageManager": "pnpm@10.14.0"
}
